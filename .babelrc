{
  "presets": ["@babel/react", "@babel/typescript", "@babel/preset-env"],
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [".js", ".jsx", ".ts", ".tsx", ".css"],
        "root": ["./src"]
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": []
    }
  }
}
